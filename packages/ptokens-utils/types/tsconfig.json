{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es6", "dom"],
    "target": "es6",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": false,
    "baseUrl": ".",
    "paths": {
      "ptokens-utils": ["."]
    }
  }
}
